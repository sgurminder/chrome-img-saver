 {

     "manifest_version" : 2,

     "name": "Image saver",
     "description": "Saves current open images to Google drive",
     "version" : "1.0",


     "browser_action" : {
	 "default_icon": "img/icon.png",
	 "default_popup": "html/popup.html",
	 "default_title": "save images to drive"
     },

     "permissions": [
	 "activeTab"
	 ],

     "content_scripts": [
	 {
	     "matches": ["<all_urls>"],
	     "js": ["js/contentScript.js"]
	 }
     ]

     

}


